[uwsgi]
chdir = /home/ubuntu/homepage
module = homepage.wsgi:application
home = /home/ubuntu/venv

uid = ubuntu
gid = ubuntu

http = :8000

enable-threads = true
master = true
vacuum = true
pidfile = /tmp/homepage.pid
logto = /var/log/uwsgi/homepage/@(exec://date +%%Y-%%m-%%d).log //로그를 저장할 장소. 로그 저장될 이 장소를 이따가 만들어줘야함
log-reopen = trued
